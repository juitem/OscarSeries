{
  "all_files": true,
  "files_sort_by": "FILESIZE",
  "files_sort_metric": "absdiff",
  "top_n_groups": 0,
  "top_n_files": 5,
  "group_sort_by": "metric",
  "group_sort_metric": "absdiff",
  "groups": {
    "FILESIZE": ["FILESIZE"],
    "CUSTOM.N_DBGMETA" : [".gnu_debugaltlink", ".gnu_debuglink", ".note.*", ".shstrtab"]
  },
  "rules": [
    {
      "if": {
        "section_flags_perms": ["AX"],
        "section_type": ["SHT_PROGBITS"],
        "addr_space": "VA"
      },
      "group": "CUSTOM.R_TEXT"
    },
    {
      "if": {
        "section_flags_perms": ["A"],
        "section_type": ["SHT_PROGBITS","SHT_GNU_HASH","SHT_GNU_verneed","SHT_GNU_versym"],
        "addr_space": "VA"
      },
      "group": "CUSTOM.R_RO"
    },
    {
      "if": {
        "section_flags_perms": ["WA"],
        "is_nobits": ["true"]
      },
      "group": "CUSTOM.R_BSS"
    },
    {
      "if": {
        "section_type": ["SHT_RELA","SHT_DYNAMIC","SHT_DYNSYM","SHT_STRTAB"],
        "addr_space": "VA"
      },
      "group": "CUSTOM.R_DYN"
    },
    {
      "if": {
        "section_flags_perms": ["WA"],
        "addr_space": "VA+PA"
      },
      "group": "CUSTOM.R_DATA"
    },
    {
      "if": {
        "section_flags_perms": ["RX"],
        "addr_space": "VA"
      },
      "group": "R.CUSTOM_RO"
    },
    {
      "if": { "section_flags_perms": "" },
      "group": "CUSTOM.R_IGNORE"
    }
  ]
}