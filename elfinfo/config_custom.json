{
  "all_files": true,
  "files_sort_by": "FILESIZE",
  "files_sort_metric": "absdiff",
  "top_n_groups": 0,
  "top_n_files": 5,
  "group_sort_by": "metric",
  "group_sort_metric": "absdiff",
  "groups": {
    "FILESIZE": ["FILESIZE"],
    "GNUWARNING": [".gnu.warning.*"]
  },
  "rules": [
    {
      "if": {
        "section_flags_perms": ["A?X", "AX"],
        "addr_space": "PA+VA"
      },
      "group": "TEXT"
    },
    {
      "if": { "section_flags_perms": "" },
      "group": "CUSTOM_IGNORE"
    },
    {
      "if": {
        "section_flags_perms": ["WA"],
        "addr_space": "PA+VA"
      },
      "group": "DATA"
    }
  ]
}