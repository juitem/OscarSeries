{
  "all_files": true,
  "files_sort_by": "FILESIZE",
  "files_sort_metric": "absdiff",
  "top_n_groups": 0,
  "top_n_files": 5,
  "group_sort_by": "metric",
  "group_sort_metric": "absdiff",
  "groups": {
    "FILESIZE": ["FILESIZE"],
    "CUSTOM_TEXT": [".text",".plt", ".init", ".fini"],
    "CUSTOM_RO" : [".rodata", ".interp", ".eh_frame", ".eh_frame_hdr", ".gnu.hash", ".gnu.version*"],
    "CUSTOM_DATA" : [".data", ".got", ".init_array", ".fini_array", ".data.rel.ro"],
    "CUSTOM_BSS" : [".bss",".tbss"],
    "CUSTOM_DYN" : [".dynsym", ".dynstr", ".rela.dyn", ".rela.plt", ".dynamic"],
    "CUSTOM_DBGMETA" : [".gnu_debugaltlink", ".gnu_debuglink", ".note.*", ".shstrtab"]
  },
  "rules": [
    {
      "if": {
        "section_flags_perms": ["A?X", "AX"],
        "addr_space": "PA+VA"
      },
      "group": "TEXT"
    },
    {
      "if": {
        "section_flags_perms": ["RX"],
        "addr_space": "VA"
      },
      "group": "CUSTOM_RODATA"
    },
    {
      "if": { "section_flags_perms": "" },
      "group": "CUSTOM_IGNORE"
    },

    {
      "if": {
        "section_flags_perms": ["WA"],
        "addr_space": "PA+VA"
      },
      "group": "DATA"
    }
  ]
}